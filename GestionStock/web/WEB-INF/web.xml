<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>EntreeStock</servlet-name>
        <servlet-class>Controllers.EntreeStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SortieStock</servlet-name>
        <servlet-class>Controllers.SortieStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EStock</servlet-name>
        <servlet-class>Controllers.EStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SStock</servlet-name>
        <servlet-class>Controllers.SStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EtatStockServlet</servlet-name>
        <servlet-class>Controllers.EtatStockServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EntreeStock</servlet-name>
        <url-pattern>/EntreeStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SortieStock</servlet-name>
        <url-pattern>/SortieStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EStock</servlet-name>
        <url-pattern>/EStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SStock</servlet-name>
        <url-pattern>/SStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EtatStockServlet</servlet-name>
        <url-pattern>/EtatStockServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
